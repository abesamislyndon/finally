<div class="container">
  <div class="row">
      <div class="col-md-12">
          <%= form_for :category, url: category_index_path do |f| %> 
            <p><%= f.label :title%> <br>
               <%= f.text_field :category_name %> <br><br>
               <%= f.submit %>
            </p>
          <% end %>
      </div>
   </div><!--end of row-->
</div>